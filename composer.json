{
    "name": "soved/laravel-ecologi",
    "description": "Help restore the planet using the Ecologi API",
    "keywords": ["laravel", "ecologi", "soved", "carbon", "trees"],
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/http": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/queue": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "Soved\\Laravel\\Ecologi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sander de Vos",
            "email": "sander@tutanota.de"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Soved\\Laravel\\Ecologi\\EcologiServiceProvider"
            ]
        }
    }
}
